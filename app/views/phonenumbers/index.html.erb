<p id="notice"><%= notice %></p>

<h1>Phone Directory</h1>

<table class="center">
  <thead>
    <tr>
      <th>Name</th>
      <th>Home</th>
      <th>Office</th>
      <th>Cell</th>
      <th>Secondary contact</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phonenumbers.each do |phonenumber| %>
      <tr>
        <td><%= phonenumber.name %></td>
        <td><%= phonenumber.home %></td>
        <td><%= phonenumber.office %></td>
        <td><%= phonenumber.cell %></td>
        <td><%= phonenumber.secondary_contact %></td>
        <td><%= link_to 'Show', phonenumber %></td>
        <td><%= link_to 'Edit', edit_phonenumber_path(phonenumber) %></td>
        <td><%= link_to 'Destroy', phonenumber, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Phone Number', new_phonenumber_path %>
