<%= form_with(model: georeport, local: true) do |form| %>
  <% if georeport.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(georeport.errors.count, "error") %> prohibited this georeport from being saved:</h2>

      <ul>
      <% georeport.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :presentaddress, "Present Address" %>
    <%= form.text_field :presentaddress %>
