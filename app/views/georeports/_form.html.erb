          <td><%= form.label :name1, "Name" %></td>
          <td><%= form.text_field :name1 %></td>
          <td><%=form.label :phone1, "Phone" %></td>
          <td><%= form.text_field :phone1 %></td>
        </tr>
        <tr>
          <td><%= form.label :name2, "Name" %></td>
